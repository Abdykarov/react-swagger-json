        "maxLength": 100,
        "pattern": "^[A-Za-z0-9]{1,100}$"
      },
      "Portfolio": {
        "type": "string",
        "enum": [
          "INTERNAL",
          "CENTRAL",
          "PRODUCTION",
          "TRADING",
          "CUSTOMER_SYSTEMS",
          "DISTRIBUTION",
          "UNKNOWN"
        ]
      },
      "OrganizationUnit": {
        "type": "object",
        "additionalProperties": false,
        "properties": {

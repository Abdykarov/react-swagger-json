            "enum": [
              "ANNOUNCEMENT",
              "INCIDENT",
              "MAJOR_INCIDENT"
            ]
          },
          "main_ci": {
            "type": "string",
            "description": "Title application or configuration item"
          },
          "main_product": {
            "type": "string",
            "description": "Title product (not applicable if main_ci is configuration item)"
          },
          "main_svc": {
            "type": "string",

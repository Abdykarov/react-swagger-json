          },
          "status": {
            "type": "string",
            "description": "Status of incident",
            "enum": [
              "OPEN",
              "CLOSED"
            ]
          },
          "root_cause": {
            "type": "string",
            "description": "Root cause of incident identified typically after incident closure"
          },
          "affected_customers": {
            "type": "array",
            "description": "List of affected customers (typically derived from affected_ci / products)",

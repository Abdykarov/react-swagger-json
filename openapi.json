                  "items": {
                    "type": "string",
                    "example": "27b97804-0d80-435c-9e86-931654960e41"
                  }
                },
                "status": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  },
                  "example": [
                    "APPROVED"
                  ]
                },
                "portfolio": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  },
                  "example": [
                    "PRODUCTION"
                  ]
                },
                "impact": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  },
                  "example": [
                    "UNAVAILABILITY"
                  ]
                },
                "start": {
                  "type": "string",
                  "format": "date-time",
                  "example": "2022-08-10T13:00:00.000Z"
                },
                "end": {
                  "type": "string",
                  "format": "date-time",
                  "example": "2022-08-11T13:00:00.000Z"
                },
                "affected_ci": {
                  "type": "array",
                  "items": {
                    "type": "number"
                  },
                  "example": [
                    21506,
                    28496
                  ]
                }
              }
            }
          }
        }
      },
      "serviceCatalogueSearch": {
        "name": "filter",
        "in": "query",
        "required": false,
        "description": "Search for given items",
        "schema": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string",
              "maxLength": 50,
              "pattern": "^[A-Za-z0-9]{3,10}$",
              "example": "OCR"
            },
            "ci": {
              "type": "array",
              "items": {
                "type": "string",
                "maxLength": 50,
                "pattern": "^[A-Za-z0-9]{3,50}$"
              },
              "example": [
                215700
              ]
            }
          }
        }
      }
    },
    "schemas": {

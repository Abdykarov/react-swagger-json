            "items": {
              "type": "object",
              "properties": {
                "title": {
                  "type": "string"
                },
                "ci": {
                  "type": "string"
                },
                "category": {
                  "type": "string",
                  "enum": [
                    "PRODUCT",
                    "SERVICE",
                    "SYSTEM",
                    "APPLICATION",
                    "CI"
                  ]
                }
              }
            }
          }
        },
